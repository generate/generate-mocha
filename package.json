{
  "name": "generate-mocha",
  "description": "Generate mocha test files.",
  "version": "0.1.5",
  "homepage": "https://github.com/generate/generate-mocha",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-mocha",
  "bugs": {
    "url": "https://github.com/generate/generate-mocha/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "lib",
    "templates"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-fs-conflicts": "^0.1.2",
    "camel-case": "^1.2.2",
    "debug": "^2.2.0",
    "extend-shallow": "^2.0.1",
    "generate-collections": "^0.2.1",
    "generate-defaults": "^0.2.0",
    "kind-of": "^3.0.2",
    "lazy-cache": "^1.0.3",
    "match-file": "^0.2.0",
    "minimist": "^1.2.0",
    "mixin-deep": "^1.1.3",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "engine-base": "^0.1.2",
    "generate": "^0.5.2",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "create",
    "generate",
    "generator",
    "init",
    "mocha",
    "template",
    "test",
    "unit",
    "yeoman"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "base",
      "generate",
      "verb"
    ],
    "run": true,
    "toc": true,
    "layout": "default",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "generate",
        "generate-collections",
        "generate-defaults",
        "generate-node"
      ]
    }
  }
}
