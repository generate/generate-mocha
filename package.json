{
  "name": "generate-mocha",
  "description": "Generate mocha test files.",
  "version": "0.1.10",
  "homepage": "https://github.com/generate/generate-mocha",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-mocha",
  "bugs": {
    "url": "https://github.com/generate/generate-mocha/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "lib",
    "templates"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-fs-conflicts": "^0.1.6",
    "base-register": "^0.1.0",
    "camel-case": "^3.0.0",
    "common-questions": "^0.2.4",
    "data-store": "^0.16.0",
    "debug": "^2.2.0",
    "generate-collections": "^0.2.1",
    "generate-defaults": "^0.2.2",
    "is-valid-app": "^0.1.1",
    "kind-of": "^3.0.3",
    "lazy-cache": "^2.0.1",
    "log-utils": "^0.1.4",
    "match-file": "^0.2.0",
    "minimist": "^1.2.0",
    "mixin-deep": "^1.1.3",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "base": "^0.11.0",
    "delete": "^0.3.2",
    "generate": "^0.7.3",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.5.3",
    "vinyl": "^1.1.1"
  },
  "keywords": [
    "create",
    "generate",
    "generator",
    "init",
    "mocha",
    "template",
    "test",
    "unit",
    "yeoman"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "base",
      "generate",
      "verb",
      "verb-readme-generator"
    ],
    "run": true,
    "toc": true,
    "layout": "global",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "generate",
        "generate-git",
        "generate-license",
        "generate-node"
      ]
    }
  }
}
